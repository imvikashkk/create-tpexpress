{
  "name": "typeexpress",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "dist/app.js",
  "scripts": {
    "db:generate": "drizzle-kit generate --config=./drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=./drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config=./drizzle.config.ts",
    "dev": "tsx --watch --env-file=.env src/app.ts",
    "build": "npm run lint && echo '> Compiling Typescript...' && tsc && tsc-alias && cpy \"src/**/*\" \"!src/**/*.ts\" dist/ --parents",
    "start": "node --env-file=.env dist/app.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "check": "npm run lint && npm run typecheck",
    "test": "node --test --env-file=.env",
    "ci": "npm ci && npm run typecheck && npm run build && npm run test",
    "prepare": "husky"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-rate-limit": "^5.1.3",
    "@types/express-session": "^1.18.2",
    "@types/express-slow-down": "^1.3.5",
    "@types/geoip-lite": "^1.4.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.0",
    "@types/pg": "^8.15.5",
    "@types/redis": "^4.0.10",
    "@types/ua-parser-js": "^0.7.39",
    "cpy-cli": "^6.0.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.34.0",
    "husky": "^8.0.0",
    "jiti": "^2.5.1",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "chalk": "^5.6.0",
    "connect-redis": "^9.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.44.5",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.2",
    "express-slow-down": "^3.0.0",
    "geoip-lite": "^1.4.10",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "redis": "^5.8.2",
    "ua-parser-js": "^2.0.5",
    "zod": "^4.1.5"
  }
}
